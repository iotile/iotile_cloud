                "5022",
                "5023"
              ],
              "displayCtrlDict": {
                "5021": {
                  "title": "Pressure",
                  "color": "#aba4a4",
                  "type": "line",
                  "axisOnTheRight": true,
                  "y_axis_range": {
                    "low": 0,
                    "high": 2000
                  }
                },
                "5022": {
                  "title": "Relative Humidity",
                  "color": "#60c2f5",
                  "type": "line",
                  "axisOnTheRight": false,
                  "y_axis_range": {
                    "low": 0,
                    "high": 100
                  }
                },
                "5023": {
                  "title": "Temperature",
                  "color": "#f98831",
                  "type": "line",
                  "axisOnTheRight": false,
                  "showLastValueBox": false,
                  "y_axis_range": {
                    "low": -40,
                    "high": 50
                  }
                }
              }
            }
          }
        }
